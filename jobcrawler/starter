<script type="text/javascript">
    $(document).ready(function () {                
        var url1 = "http://i.jobkorea.kr/content/js/sidebar.min.js?date=20170529";
        var url2 = "http://i.jobkorea.kr/content/js/starter/jquery.nanoscroller.js?date=20170529";
        var url3 = "/Scripts/SideBar/sidebar.js?date=20170823";

        $.getScript( url1, function( data, textStatus, jqxhr ) {
            jkSideBar.landingBanner('.landingBannerSimple');                
            jkSideBar.init();                            
        });
        $.getScript( url2, function( data, textStatus, jqxhr ) {});
        $.getScript( url3, function( data, textStatus, jqxhr ) {});

        $(".recentLayer").on("click", function (e) {            
            $.ajax({
                type: 'post',
                url: '/SideBar/RightBanRecentAjax',
                dataType: 'html',
                cache: false,                
                success: function (data) {                                        
                    $("#recentList").html(data);
                    jkSideBar.dataComplete();
                }, error: function (e) {

                }
            });            
        });

        $(".scrapLayer").on("click", function (e) {            
            $.ajax({
                type: 'post',
                url: '/SideBar/RightBanScrapAjax',
                dataType: 'html',
                cache: false,                
                success: function (data) {                    
                    $("#scrapList").html(data);
                    jkSideBar.dataComplete();
                }, error: function (e) {

                }
            });            
        });        
    });

    // 제안하기 팝업
    $('#suggestTab').on("click", function (e) {
        if (e.preventDefault) {
            e.preventDefault()
        } else {
            e.stop()
        };

        e.returnValue = false;
        e.stopPropagation();

        var windowW = 770;  // 창의 가로 길이
        var windowH = 650;  // 창의 세로 길이
        var left = Math.ceil((window.screen.width - windowW) / 2);
        var top = Math.ceil((window.screen.height - windowH) / 2);

        window.open("/Help/Suggest/Index?referrer=" + encodeURIComponent(document.location.href), "SuggestPopup", "toolbar=no,scrollbars=no,directories=no,menubar=no,width=770,height=650,top=" + top + ",left=" + left);
    });

</script>


