    <script type="text/javascript">
        // TEST
        $(document).ready(function () {
            $('.btnOnairClose').click(function () {
                $('.lyOnAirNoticeBx').hide();
                closeNnLayer();
            })

            function closeNnLayer() {
                setCookie3("Line", "done", 365);
            }

            function setCookie3(name, value, expiredays) {
                var todayDate = new Date();
                todayDate.setDate(todayDate.getDate() + expiredays);
                document.cookie = name + "=" + escape(value) + "; path=/; expires=" + todayDate.toGMTString() + ";"
            }

            cookiedata1 = document.cookie;

            if (cookiedata1.indexOf("Line=done") < 0) {
                $('.lyOnAirNoticeBx').show();
            } else {
                $('.lyOnAirNoticeBx').hide();
            }
        })
    </script>



