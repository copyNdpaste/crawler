<script type="text/javascript">
    // 공공기관 채용일정 메뉴의 Live공채소식의 Update 알림 아이콘 제거
    $(function () {
        setTimeout(function () {
            $(".icnNavNew").parent("a").each(function () {
                var sUrl = $(this).attr("href").toLowerCase();
                if (sUrl.indexOf("/starter/live/View/29986?LinkGbn=1".toLowerCase()) >= 0) {
                    $(this).find(".icnNavNew").remove();
                }
            });
        }, 1500);
    });
</script>



