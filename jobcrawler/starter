    <script type="text/javascript">
        //Layer SMS 발송처리
        function layerSendSms() {
            var f = layerSmsForm;
            var input_str = f.Mobile.value;
            var result = true;
            var char_cnt = 0;

            if ((input_str == "") || (input_str == "휴대폰번호 입력( - 생략)")) {
                alert("휴대폰 번호를 입력해주세요.");
                return false;
            }

            for (var i = 0; i < input_str.length; i++) {
                if ((input_str.charAt(i) >= '0' && input_str.charAt(i) <= '9') || (input_str.charAt(i) == '-')) {
                    char_cnt++;
                }
                else {
                    alert("숫자와 '-' 만 입력해주세요.");
                    return false;
                    break;
                }
            }

            var $agree = $("#lb_sms");
            if (!$agree.prop("checked")) {
                alert("개인정보이용에 동의해 주세요.");
                $agree.focus();
                return false;
            }

            jQuery.ajax({
                url: "/Starter/Default/AppSmsSend",
                data: "mobile=" + input_str + "&strmsg=취업 준비의 시작 잡코리아라서 다행이에요! 앱 다운로드 : joburl.kr/8LNj3",
                contentType: "application/x-www-form-urlencoded;charset=utf-8",
                type: "POST",
                dataType: "text",
                success: function (result) {
                    if (result > 0) {
                        alert("문자메시지를 발송하였습니다.");
                        resumeCountUp(86140);
                        setCookie2("Notice", "done", 364);
                        $(".btnDnClose").trigger("click");
                    } else {
                        alert("문자 발송중 오류가 발생하였습니다.");
                    }
                }
            });
            return false;
            //document.getElementById("devIfrm").contentWindow.location.href = "http://mobile.jobkorea.co.kr/include/Mobile_SVC_SMS_Send.asp?SVC_URL=joburl.kr/8LNj3&Mobile=" + input_str;
        }

        $(function () {
            $(".descViewLink .agreeDiv").on("click", function (e) {
                e.preventDefault();
                var $ele = $(".lyAgreeInfo");
                if ($ele.is(":hidden")) {
                    $(this).html("<span>내용닫기</span>");
                    $ele.show();
                } else {
                    $(this).html("<span>내용보기</span>");
                    $ele.hide();
                }
            });
            $(".close button").click(function (e) {
                $(".descViewLink .agreeDiv").trigger("click");
            });
        });

        function layerShow() {
            $('.beforeBx').hide();
            $('.afterBx').show();
            resumeCountUp(86139);
        }

        function closelayer() {
            setCookie2("Notice", "done", 365);
            $('.jkLyAppDown').hide();
        }

        function setCookie2(name, value, expiredays) {
            var todayDate = new Date();
            todayDate.setDate(todayDate.getDate() + expiredays);
            document.cookie = name + "=" + escape(value) + "; path=/; expires=" + todayDate.toGMTString() + ";"
        }

        $(document).ready(function () {
            cookiedata = document.cookie;

            if (cookiedata.indexOf("Notice=done") < 0) {
                //$('.jkLyAppDown').show();
                //resumeCountUp(86138);
            } else {
                $('.jkLyAppDown').hide();
            }
        });

        function resumeCountUp(type) {
            var logPath = "http://m.jobkorea.co.kr/Link/?No=" + type;
            var callObject = $.ajax({
                url: logPath,
                method: "post",
                dataType: "jsonp"
            });
        }

    </script>


