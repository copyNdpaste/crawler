<script type="text/javascript">
    //신입공채 속보 받아보기 설정
    function TurnOnStarterRecruitCommonPush() {
        var logined = false;

        if(!logined){
            $(".sideStarNews .link").addClass("noData");
            //로그인 레이어 
            starterAction.calendarScrapView.viewScrap($(".sideStarNews"), function () {                       
                window.location.reload();                        
            });     
        }
        else{
            //속보받기 설정
            jQuery.ajax({
                type: "post",
                async: false,
                url: "/starter/SetCommonPCPush",
                success: function (data) {
                    $(".sideStarNews .link").removeClass("noData");                      
                }
            });
        }
    }

</script>

